{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Run auto-trade",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/src/auto_trading_bot.ts",
        "runtimeArgs": ["-r", "ts-node/register"],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }